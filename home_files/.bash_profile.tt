# --Start iTerm Color--

iterm_profile() {
  # Set tab color to red
  echo -e "\033]6;1;bg;red;brightness;255\a"
  #Set window profile
  echo -e '\033]50;SetProfile='$1'\a' ;
}

color_ssh() {
  iterm_profile SSH
  ssh $@
  iterm_profile Default
}

